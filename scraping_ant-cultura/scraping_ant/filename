ow.location.hash)\n             */\n            // edge case: hash in URL but hash is empty\n            var hash = window.location.hash.length == 0 ? '#' : window.location.hash;\n            // edge case: question mark in URL but search is empty \n            var currentSearch = window.location.search.length == 0 ? '?' : window.location.search;\n\n            var redirectUrl = window.location.href.replace(hash, '').replace(currentSearch, ''); \n            redirectUrl += '?' + search;\n            // re-add hash here if need be\n\n            CulturaLayer.prototype.selectFacetUrl(redirectUrl);\n        })\n\n        function updateCounter(values) {\n\n            countProducts = 0;\n\n            for (var i=0; i < config.allowedIntervals.length; i++) {\n                var currentValue = config.allowedIntervals[i].value;\n\n                if (values[0] <= currentValue && values[1] >= currentValue) {\n                    countProducts += config.allowedIntervals[i].count;\n                }\n            }\n\n            counterNode.innerHTML = countProducts;\n\n            countTemplate = 'empty';\n\n            if (countProducts > 1) {\n                countTemplate = 'multiple';\n                validButton.show();\n            } else if (countProducts > 0) {\n                countTemplate = 'one';\n                validButton.show();\n            } else {\n                validButton.hide();\n            }\n\n            counterNode.innerHTML = config.countProductTemplates[countTemplate].evaluate({count: countProducts})\n            counterNode.removeClassName('one'); counterNode.removeClassName('multiple'); counterNode.removeClassName('empty');\n            counterNode.addClassName(countTemplate);\n        }\n\n        Event.observe(window, 'resize', initSlider);\n        onSliderChangeValues(slider.values);\n\n    })(sliderConfig);\n</script></div>\n<div class=\"h3 \">Avis</div>\n<div class=\"narrow-by-list-filter-values\"><ol>\n<li class=\"rating-filter\">\n<a